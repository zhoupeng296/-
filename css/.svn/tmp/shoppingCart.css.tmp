/* 全部商品 */
#all{
    margin-top: 30px;
    min-height: 450px;
    /* background: green; */
}
#all>.wrap>p{
    color: #666666;
}
#all .top{
    width: 1200px;
    height: 60px;
    background: #f2f2f2;
}
#all .top>.left{
    width: 326px;
    height: 40px;
    margin-top: 22px;
}
#all .top>.left>input{
    width: 18px;
    height: 18px;
    margin-left: 23px;
    vertical-align: middle;
}
#all .top>.left>.right{
    margin-right: 75px;
}
#all .top>.left>span{
    color: #808080;
    font-size: 18px;
}
#all .top>.right{
    width: 558px;
    height: 60px;
    color: #808080;
    margin-top: 22px;
    margin-left: 39px;
}
#all .top>.right>span{
    float: right;
    margin-right: 89px;
}
/* 结算界面 */
#all .bottom{
    width: 1200px;
    /* height: 423px; */
    margin-top: 25px;
    border-top: 5px solid #ccc;
}
#all .bottom>.joinMenu{
    width: 1200px;
    margin-top: 26px;
    padding-bottom: 30px;
    border-bottom: 2px solid #ccc;
}
#all .bottom>.joinMenu>.left>input{
    margin-top: 27px;
    margin-left: 23px;
    width: 18px;
    height: 18px;
    vertical-align: middle;
}
#all .bottom>.joinMenu>.left>.img{
    width: 150px;
    height: 150px;
    text-align: center;
    margin-top: 27px;
    margin-left: 33px;
    margin-right: 29px;
    border: 1px solid #e6e6e6;
}
#all .bottom>.joinMenu>.right{
    width: 970px;
    height: 44px;
    padding-top: 6px;
    margin-right: 24px;
    color: #4d4d4d;
}
#all .bottom>.joinMenu>.right>.right input{
    width: 74px;
    height: 24px;
    text-indent: 34px;
    border: 1px solid #ccc;
}
#all .bottom>.joinMenu>.right>.right button{
    width: 26px;
    height: 26px;
    border: 1px solid #ccc;
}
#all .bottom>.joinMenu>.right>.right>.price{
    margin-right: 44px;
}
#all .bottom>.joinMenu>.right>.right>div:nth-child(2){
    margin-right: 35px;
}
#all .bottom>.joinMenu>.right>.right>span:nth-child(3){
    margin-right: 85px;
}
/* 结算 */
#all .bottom>.close{
    width: 1200px;
    height: 60px;
    margin-top: 32px;
    line-height: 60px;
    color: #4d4d4d;
}
#all .bottom>.close>.left{
    width: 1070px;
    height: 58px;
    border: 1px solid #ccc;
}
#all .bottom>.close>.right{
    width: 128px;
    height: 60px;
    background: #f46102;
    border: 0;
}
#all .bottom>.close>.left>.left>input{
    width: 18px;
    height: 18px;
    margin-left: 23px;
    vertical-align: middle;
    margin-right: 11px;
}
#all .bottom>.close>.left>.left>span:nth-child(2){
    margin-right: 33px;
}
#all .bottom>.close>.left>.right>.r{
    margin-left: 50px;
}
#all .bottom>.close>.left>.right>.r{
    margin-right: 60px;
}
#all .bottom>.close>.left>.right>div:first-child>span:nth-child(2){
    margin: 0 10px;
}
#fond>.wrap>.top{
