/* 导航 */
#nav .left{
    width: 200px;
    height: 528px;
    /* border: 1px solid #000; */
    position: relative;
    background:#efefef;
}
#nav .left .logo{
    position: absolute;
    top:-28px;
    width: 200px;
    height: 130px;
    background: url(../images/LOGO.jpg) no-repeat;
}
#nav .left .domul{
    margin-top: 110px;
    /* margin-left:20px; */
    line-height: 28px;
}
#nav .left .domul li{
    padding-left: 20px;
    border: 1px solid transparent;
}
#nav .left .domul li:hover{
    border: 1px solid #ff6700;
    border-right: 1px solid transparent;
    width: 180px;
    background-color: white;
    color:#ff6700;
    z-index: 2;
    position: relative;
}
/* 二级列表 */
#nav .left .li2 div{
    width: 540px;
    height: 460px;
    background:white;
    border: 1px solid #ff6700;
    z-index:1;
    position: absolute;
    top:101px;
    left:200px;
    padding-left: 30px;
    display:none;
    padding-top: 40px;
}

#nav .left .li2 .active{
    display:block;
}
.borderwhite{
    border: 1px solid #ff6700;
    border-right: 1px solid  white;
}
#nav .left .li2 li{

    height: 55px;
}






/* 搜索框与购物车 */
#nav .leftTop{
    height: 84px;
    width: 988px;
    margin-top: 10px;
    margin-bottom: 8px;

    /* border: 1px solid #000; */
}
#nav .leftTop .search{
    width: 488px;
    margin-top: 4px;
    margin-left: 270px;
}
#nav .leftTop input{
    width: 430px;
    height: 34px;
    border: 2px solid #ff6700;
    padding-left: 6px;
}
#nav .leftTop button{
    width: 48px;
    height: 38px;
    background: #ff6700 url(../images/bgicon.png) ;
    border:none;
}
#nav .shoppingCar{
    width: 200px;
}
#nav .shoppingCar div{
    margin-top: 4px;
    line-height: 38px;
    text-align: center;
    width: 100px;
    height: 38px;
}
#nav .shoppingCar .fl{
    text-indent: 2em;
    color: white;
    background: #ff6700 url(../images/01-首页_03.png) no-repeat;
}
#nav .shoppingCar .fr{
    background:#ebebeb;
}

#nav .leftTop ul{
    width: 990px;
}
#nav .leftTop ul li{
    margin-top: 14px;
    float: left;
    margin-left:40px;
    font-weight: bold;
}

/* 今日推荐 */
#recommend{
    margin-top: 10px;
}
#recommend p:nth-child(2){
    width: 1200px;
    height: 4px;
    background-color: #ff6700;
}
#recommend ul{
    border-left:1px solid #ff6700;
    margin-left: 12px;
    margin-bottom: 40px;
}
#recommend ul li{
    width: 195px;
    height: 250px;
    float: left;
    border-bottom: 1px solid #ff6700;
    border-right: 1px solid #ff6700 ;
}
/* 新书 电子书 */
#newBook>p:nth-child(2){
    width: 888px;
    height: 4px;
    background-color: #ff6700;
}
#newBook .left{
    width: 236px;
}
#newBook .left ul{
    background: #f9f9f9;
    padding-bottom: 5px;
}
#newBook .left li{
    float: left;
    width: 70px;
    margin:10px 4px;
}
/* table切换 */
#newBook .bookTable img{
    width: 158px;
    height: 116px;
}
#newBook .middle{
    width: 660px;
    position: relative;

}
#newBook .middle .nav{
    width: 324px;
    height: 215px;
    border: 1px solid #cccccc;
}
#newBook .middle .shoopBook li{
    height: 216px;
    width: 162px;
    border-bottom: 1px solid #cccccc;
    border-right: 1px solid #cccccc ;
    float: left;
}
#newBook .middle .bookNav{
    position: absolute;
    top:-32px;
    right:8px;
}
#newBook .middle .bookNav li{
    height: 32px;
    width: 100px;
    float: left;
    text-align: center;
    line-height: 32px;
}
.shoopBook{
    display: none;
}
.newBookactive{
    background-color: #ff6700;
    color: white;
}
.shoopBookactive{
    display:block !important;
}
.displayNone{
    display: none;
}
.displayBlock{
    display:block;
}
/* 手风琴 */
#newBook .right{
    width: 282px;
    border: 1px solid #cccccc;
    height: 432px;
    overflow: hidden;
}
#newBook .right .change2 span:last-child{
    width: 120px;
    margin-right: 20px;
    margin-top: 20px;
}
#newBook .right .change1{
    height: 42px;
}






