/* 动态搜索 */
#searchBox{
    width: 100%;
    background-color: rgba(122, 116, 116, 0.5);
    height: 50px;
    position: fixed;
    top:0px;
    right:0;
    text-align: center;
    border-bottom: 5px solid #ff6700;
}
#searchBox .wrap>div{
    width: 900px;
    height: 50px;

}
#searchBox .wrap>div>p{
    color: white;
    font-size: 20px;
    line-height: 50px;
    margin-left: 100px;
}

#searchBox input{
    width: 430px;
    height: 34px;
    border:none;
    padding-left: 6px;
    margin-top: 6px;

}
#searchBox  button{
    width: 48px;
    height: 34px;
    background: #fb3904 url(../images/bgicon.png) ;
    border:none;
    margin-top: 6px;
}

